{
  "tsserver.implicitProjectConfig.checkJs": false,
  "tsserver.reportStyleChecksAsWarnings": false,
  "tsserver.orgnizeImportOnSave": false,
  "tsserver.enableJavascript": true,
  "tsserver.npm": "/usr/local/bin/npm",
  "typescript.suggest.paths": true,
  "typescript.suggest.autoImports": true,
  "typescript.updateImportsOnFileMove.enable": true,
  "typescript.suggest.completeFunctionCalls": true,
  "typescript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "javascript.suggest.paths": true,
  "javascript.preference.quoteStyle": "source",
  "vetur.experimental.templateInterpolationService": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "tsx",
    "ts",
    "js",
    "less",
    "md",
    "jsx",
    "json"
  ],
  "prettier.requireConfig": true,
  "prettier.semi": true,
  "prettier.arrowParens": "always",
  "prettier.printWidth": 80,
  "prettier.bracketSpacing": true,
  "prettier.jsxBracketSameLine": true,
  "prettier.trailingComma": "none",
  "prettier.singleQuote": true,
  "prettier.tslintIntegration": true,
  "prettier.useTabs": true,
  "json.format.enable": true,
  "coc.source.emoji.enable": true,
  "coc.preferences.diagnostic.enable": false,
  "suggest.enablePreview":true
}
