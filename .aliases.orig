# Function that renames a file by adding a suffix before the extension

renamesuffix() {
  if [ "$#" -ne 2 ]; then
    echo "Usage: renamesuffix <input_file> <suffix>"
    return 1
  fi

  input_file="$1"
  suffix="$2"

  base="${input_file%.*}"          # Remove extension from input_file
  dir=$(dirname "$input_file")     # Get directory path
  filename=$(basename "$base")     # Get just the base filename (no path)

  fs_file="${dir}/${filename}.funscript"  # Full path to .fs file
  newname="${filename} ${suffix}.funscript"  # New name (filename only)

  if [ ! -f "$fs_file" ]; then
    echo "Error: '$fs_file' not found."
    return 1
  fi

  mv -- "$fs_file" "${dir}/${newname}"
  echo "Renamed to: ${dir}/${newname}"
}




# Enable tab completion for the first argument (filename)
complete -f -o default renamesuffix

# Function that always appends "AIScript" as the suffix
rnm() {
  if [ "$#" -ne 1 ]; then
    echo "Usage: rnm <filename>"
    return 1
  fi
  renamesuffix "$1" "AIScript"
}

# Enable filename tab completion
complete -f -o default rnm
